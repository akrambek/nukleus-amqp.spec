property serverTransport "nukleus://streams/target#0"

accept ${serverTransport}
  option nukleus:window 8192
  option nukleus:transmission "duplex"

accepted
connected

# header exchange
read "AMQP" [0x00 0x01 0x00 0x00]
write "AMQP" [0x00 0x01 0x00 0x00]

# open frame exchange
read [0x00 0x00 0x00 0x23]                                        # length = 35
     [0x02]                                                       # doff = 2
     [0x00]                                                       # type = AMQP (0)
     [0x00 0x00]                                                  # channel = 0
     [0x00 0x53 0x10]                                             # performative = open (16)
     [0xc0 0x16]                                                  # list type (LIST1), length = 22
     [0x03]                                                       # field count = 3
     [0xa1 0x06] "client"                                         # container-id = "client"
     [0xa1 0x06] "server"                                         # hostname = "server"
     [0x70 0x00 0x10 0x00 0x00]                                   # max frame size = 1048576

write [0x00 0x00 0x00 0x1c]                                       # length = 28
      [0x02]                                                      # doff = 2
      [0x00]                                                      # type = AMQP (0)
      [0x00 0x00]                                                 # channel = 0
      [0x00 0x53 0x10]                                            # performative = open (16)
      [0xc0 0x0f]                                                 # list type (LIST1), length = 15
      [0x03]                                                      # field count = 3
      [0xa1 0x06] "server"                                        # container-id = "server"
      [0x40]                                                      # hostname = null
      [0x70 0x00 0x02 0x00 0x00]                                  # max frame size = 131072

# begin frame exchange
read [0x00 0x00 0x00 0x1b]                                        # length = 27
     [0x02]                                                       # doff = 2
     [0x00]                                                       # type = AMQP (0)
     [0x00 0x01]                                                  # channel = 1
     [0x00 0x53 0x11]                                             # performative = begin (17)
     [0xc0 0x0e]                                                  # list type (list8), length = 14
     [0x04]                                                       # field count = 4
     [0x40]                                                       # remote-channel = null
     [0x52 0x01]                                                  # next-outgoing-id = 1
     [0x70 0x00 0x00 0x07 0xff]                                   # incoming-window = 2047
     [0x70 0x7f 0xff 0xff 0xff]                                   # outgoing-window = 2147483647

write [0x00 0x00 0x00 0x1a]                                       # length = 26
      [0x02]                                                      # doff = 2
      [0x00]                                                      # type = AMQP (0)
      [0x00 0x01]                                                 # channel = 1
      [0x00 0x53 0x11]                                            # performative = begin (17)
      [0xc0 0x0d]                                                 # list type (list8), length = 13
      [0x04]                                                      # field count = 4
      [0x60 0x00 0x01]                                            # remote-channel = 1
      [0x52 0x01]                                                 # next-outgoing-id = 1
      [0x52 0x02]                                                 # incoming-window = 2
      [0x70 0x7f 0xff 0xff 0xff]                                  # outgoing-window = 2147483647

# attach frame exchange
read [0x00 0x00 0x00 0x31]                                        # length = 49
     [0x02]                                                       # doff = 2
     [0x00]                                                       # type = AMQP (0)
     [0x00 0x01]                                                  # channel = 1
     [0x00 0x53 0x12]                                             # performative = attach (18)
     [0xc0 0x24]                                                  # list type (list8), length = 36
     [0x0a]                                                       # field count = 10
     [0xa1 0x05] "link1"                                          # name = "link1"
     [0x43]                                                       # handle = 0
     [0x42]                                                       # role = sender
     [0x50 0x01]                                                  # snd-settle-mode = settled
     [0x50 0x00]                                                  # rcv-settle-mode = first
     [0x00 0x53 0x28]                                             # source
     [0xc0 0x0a]                                                  # list type (list8), length = 10
     [0x01]                                                       # field count = 1
     [0xa1 0x07] "clients"                                        # address = "clients"
     [0x00 0x53 0x29 0x45]                                        # target = empty list
     [0x40]                                                       # unsettled = null
     [0x40]                                                       # incomplete-unsettled = null (default = false)
     [0x43]                                                       # initial-delivery-count = 0

write [0x00 0x00 0x00 0x2a]                                       # length = 42
      [0x02]                                                      # doff = 2
      [0x00]                                                      # type = AMQP (0)
      [0x00 0x01]                                                 # channel = 1
      [0x00 0x53 0x12]                                            # performative = attach (18)
      [0xc0 0x1d]                                                 # list type (list8), length = 29
      [0x06]                                                      # field count = 6
      [0xa1 0x05] "link1"                                         # name = "link1"
      [0x43]                                                      # handle = 0
      [0x41]                                                      # role = sender
      [0x50 0x01]                                                 # snd-settle-mode = settled
      [0x50 0x00]                                                 # rcv-settle-mode = first
      [0x00 0x53 0x28]                                            # source
      [0xc0 0x0a]                                                 # list type (list8), length = 10
      [0x01]                                                      # field count = 1
      [0xa1 0x07] "clients"                                       # address = "clients"

# flow frame
write [0x00 0x00 0x00 0x20]                                       # length = 32
      [0x02]                                                      # doff = 2
      [0x00]                                                      # type = AMQP (0)
      [0x00 0x01]                                                 # channel = 1
      [0x00 0x53 0x13]                                            # performative = flow (19)
      [0xc0 0x13]                                                 # list type (list8), length = 19
      [0x07]                                                      # field count = 7
      [0x52 0x01]                                                 # next-incoming-id = 1
      [0x52 0x02]                                                 # incoming-window = 2
      [0x52 0x01]                                                 # next-outgoing-id = 1
      [0x70 0x7f 0xff 0xff 0xff]                                  # outgoing-window = 2147483647
      [0x43]                                                      # handle
      [0x43]                                                      # delivery-count = 0
      [0x70 0x00 0x00 0x03 0xe8]                                  # link-credit = 1000

# transfer frame
read [0x00 0x00 0x00 0x15]                                        # length = 21
     [0x02]                                                       # doff = 2
     [0x00]                                                       # type = AMQP (0)
     [0x00 0x01]                                                  # channel = 1
     [0x00 0x53 0x14]                                             # performative = transfer (20)
     [0xc0 0x08]                                                  # list type (list8), length = 8
     [0x05]                                                       # field count = 5
     [0x43]                                                       # handle = 0
     [0x43]                                                       # delivery-id = 0
     [0xa0 0x01 0x00]                                             # delivery-tag = 00
     [0x43]                                                       # message-format = 0
     [0x41]                                                       # settled = true
     [0x00 0x53 0x77 0xa1 0x05] "Text1"                           # payload = "Text1"

# transfer frame
read [0x00 0x00 0x00 0x15]                                        # length = 21
     [0x02]                                                       # doff = 2
     [0x00]                                                       # type = AMQP (0)
     [0x00 0x01]                                                  # channel = 1
     [0x00 0x53 0x14]                                             # performative = transfer (20)
     [0xc0 0x08]                                                  # list type (list8), length = 8
     [0x05]                                                       # field count = 5
     [0x43]                                                       # handle = 0
     [0x52 0x01]                                                  # delivery-id = 1
     [0xa0 0x01 0x01]                                             # delivery-tag = 01
     [0x43]                                                       # message-format = 0
     [0x41]                                                       # settled = true
     [0x00 0x53 0x77 0xa1 0x05] "Text2"                           # payload = "Text2"

# end frame
write [0x00 0x00 0x00 0x33]                                       # length = 51
      [0x02]                                                      # doff = 2
      [0x00]                                                      # type = AMQP (0)
      [0x00 0x01]                                                 # channel = 1
      [0x00 0x53 0x17]                                            # performative = end (23)
      [0xc0 0x26]                                                 # list type (list8), length = 38
      [0x01]                                                      # field count = 1
      [0x00 0x53 0x1d]                                            # error header
      [0xc0 0x20]                                                 # list type (list8), length = 32
      [0x01]                                                      # field count = 1
      [0xa3 0x1d] "amqp:session:window-violation"                 # error type, length = 29

read [0x00 0x00 0x00 0x0c]                                        # length = 12
     [0x02]                                                       # doff = 2
     [0x00]                                                       # type = AMQP (0)
     [0x00 0x01]                                                  # channel = 1
     [0x00 0x53 0x17]                                             # performative = end (23)
     [0x45]                                                       # list type (list0)
