connect await ROUTED_SERVER
        "nukleus://streams/amqp#0"
  option nukleus:window 8192
  option nukleus:transmission "duplex"

connected

# header exchange
write "AMQP" [0x00 0x01 0x00 0x00]
read "AMQP" [0x00 0x01 0x00 0x00]

# open frame exchange
write [0x00 0x00 0x00 0x16]                                      # length = 22
      [0x02]                                                     # doff = 2
      [0x00]                                                     # type = AMQP (0)
      [0x00 0x00]                                                # channel = 0
      [0x00 0x53 0x10]                                           # performative = open (16)
      [0xc0 0x09]                                                # list type (LIST1), length = 9
      [0x01]                                                     # field count = 1
      [0xa1 0x06] "client"                                       # container-id = "client"

read [0x00 0x00 0x00 0x16]                                       # length = 22
     [0x02]                                                      # doff = 2
     [0x00]                                                      # type = AMQP (0)
     [0x00 0x00]                                                 # channel = 0
     [0x00 0x53 0x10]                                            # performative = open (16)
     [0xc0 0x09]                                                 # list type (LIST1), length = 9
     [0x01]                                                      # field count = 1
     [0xa1 0x06] "server"                                        # container-id = "server"

# begin frame exchange
write [0x00 0x00 0x00 0x1b]                                      # length = 27
      [0x02]                                                     # doff = 2
      [0x00]                                                     # type = AMQP (0)
      [0x00 0x01]                                                # channel = 1
      [0x00 0x53 0x11]                                           # performative = begin (17)
      [0xc0 0x0e]                                                # list type (list8), length = 14
      [0x04]                                                     # field count = 4
      [0x40]                                                     # remote-channel = null
      [0x52 0x01]                                                # next-outgoing-id = 1
      [0x70 0x00 0x00 0x07 0xff]                                 # incoming-window = 2047
      [0x70 0x7f 0xff 0xff 0xff]                                 # outgoing-window = 2147483647

read [0x00 0x00 0x00 0x1d]                                       # length = 29
     [0x02]                                                      # doff = 2
     [0x00]                                                      # type = AMQP (0)
     [0x00 0x01]                                                 # channel = 1
     [0x00 0x53 0x11]                                            # performative = begin (17)
     [0xc0 0x10]                                                 # list type (list8), length = 16
     [0x04]                                                      # field count = 4
     [0x60 0x00 0x01]                                            # remote-channel = 1
     [0x52 0x01]                                                 # next-outgoing-id = 1
     [0x70 0x00 0x01 0x00 0x00]                                  # incoming-window = 65536
     [0x70 0x7f 0xff 0xff 0xff]                                  # outgoing-window = 2147483647

# attach frame exchange
write [0x00 0x00 0x00 0x1d]                                      # length = 29
      [0x02]                                                     # doff = 2
      [0x00]                                                     # type = AMQP (0)
      [0x00 0x01]                                                # channel = 1
      [0x00 0x53 0x12]                                           # performative = attach (18)
      [0xd0 0x00 0x00 0x00 0x0d]                                 # list type (list32), length = 13
      [0x00 0x00 0x00 0x03]                                      # field count = 3
      [0xa1 0x05] "link1"                                        # name = "link1"
      [0x43]                                                     # handle = 0
      [0x42]                                                     # role = sender

read [0x00 0x00 0x00 0x1d]                                       # length = 29
     [0x02]                                                      # doff = 2
     [0x00]                                                      # type = AMQP (0)
     [0x00 0x01]                                                 # channel = 1
     [0x00 0x53 0x12]                                            # performative = attach (18)
     [0xd0 0x00 0x00 0x00 0x0d]                                  # list type (list32), length = 13
     [0x00 0x00 0x00 0x03]                                       # field count = 3
     [0xa1 0x05] "link1"                                         # name = "link1"
     [0x43]                                                      # handle = 0
     [0x41]                                                      # role = receiver

# flow frame
read [0x00 0x00 0x00 0x23]                                       # length = 35
     [0x02]                                                      # doff = 2
     [0x00]                                                      # type = AMQP (0)
     [0x00 0x01]                                                 # channel = 1
     [0x00 0x53 0x13]                                            # performative = flow (19)
     [0xc0 0x16]                                                 # list type (list8), length = 22
     [0x07]                                                      # field count = 7
     [0x52 0x01]                                                 # next-incoming-id = 1
     [0x70 0x00 0x00 0x3f 0xff]                                  # incoming-window = 16383
     [0x52 0x01]                                                 # next-outgoing-id = 1
     [0x70 0x7f 0xff 0xff 0xff]                                  # outgoing-window = 2147483647
     [0x43]                                                      # handle = 0
     [0x43]                                                      # delivery-count = 0
     [0x70 0x00 0x00 0x00 0x00]                                  # link-credit = 0

# transfer frame
write [0x00 0x00 0x00 0x15]                                       # length = 21
      [0x02]                                                      # doff = 2
      [0x00]                                                      # type = AMQP (0)
      [0x00 0x01]                                                 # channel = 1
      [0x00 0x53 0x14]                                            # performative = transfer (20)
      [0xc0 0x08]                                                 # list type (list8), length = 8
      [0x05]                                                      # field count = 5
      [0x43]                                                      # handle = 0
      [0x43]                                                      # delivery-id = 0
      [0xa0 0x01 0x30]                                            # delivery-tag = "0"
      [0x43]                                                      # message-format = 0
      [0x41]                                                      # settled = true

# detach frame exchange
read [0x00 0x00 0x00 0x33]                                        # length = 51
     [0x02]                                                       # doff = 2
     [0x00]                                                       # type = AMQP (0)
     [0x00 0x01]                                                  # channel = 1
     [0x00 0x53 0x16]                                             # performative = detach (22)
     [0xc0 0x26]                                                  # list type (list8), length = 38
     [0x03]                                                       # field count = 3
     [0x43]                                                       # handle = 0
     [0x41]                                                       # closed = true
     [0xa3 0x21] "amqp:link:transfer-limit-exceeded"              # error = transfer-limit-exceeded

write [0x00 0x00 0x00 0x33]                                       # length = 51
      [0x02]                                                      # doff = 2
      [0x00]                                                      # type = AMQP (0)
      [0x00 0x01]                                                 # channel = 1
      [0x00 0x53 0x16]                                            # performative = detach (22)
      [0xc0 0x26]                                                 # list type (list8), length = 38
      [0x03]                                                      # field count = 3
      [0x43]                                                      # handle = 0
      [0x41]                                                      # closed = true
      [0xa3 0x21] "amqp:link:transfer-limit-exceeded"             # error = transfer-limit-exceeded
