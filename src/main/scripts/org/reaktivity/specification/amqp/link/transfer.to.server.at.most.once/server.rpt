property serverTransport "nukleus://streams/target#0"

accept ${serverTransport}
  option nukleus:window 8192
  option nukleus:transmission "duplex"

accepted
connected

# header exchange
read "AMQP" [0x00 0x01 0x00 0x00]
write "AMQP" [0x00 0x01 0x00 0x00]

# open frame exchange
read [0x00 0x00 0x00 0x18]                                        # length = 24
     [0x02]                                                       # dof = 2
     [0x00]                                                       # type = AMQP (0)
     [0x00 0x00]                                                  # channel = 0
     [0x00 0x53 0x10]                                             # performative = open (16)
     [0xd0 0x00 0x00 0x00 0x08]                                   # list type (list32), physical length = 8
     [0x00 0x00 0x00 0x01]                                        # number of fields in list = 1
     [0xa1 0x02 0x63 0x31]                                        # container-id = "c1"

write [0x00 0x00 0x00 0x19]                                       # length = 25
      [0x02]                                                      # dof = 2
      [0x00]                                                      # type = AMQP (0)
      [0x00 0x00]                                                 # channel = 0
      [0x00 0x53 0x10]                                            # performative = open (16)
      [0xc0 0x0c]                                                 # list type (list8), physical length = 12
      [0x01]                                                      # number of fields in list = 1
      [0xa1 0x09 0x6c 0x6f 0x63 0x61 0x6c 0x68 0x6f 0x73 0x74]    # container-id = "localhost"

# begin frame exchange
read [0x00 0x00 0x00 0x1b]                                        # length = 27
     [0x02]                                                       # dof = 2
     [0x00]                                                       # type = AMQP (0)
     [0x00 0x01]                                                  # channel = 1
     [0x00 0x53 0x11]                                             # performative = begin (17)
     [0xc0 0x0e]                                                  # list type (list8), physical length = 14
     [0x04]                                                       # number of fields in list = 4
     [0x40]                                                       # remote-channel = null
     [0x52 0x01]                                                  # next-outgoing-id = 1
     [0x70 0x00 0x00 0x07 0xff]                                   # incoming-window = 2047
     [0x70 0x7f 0xff 0xff 0xff]                                   # outgoing-window = 2147483647

write [0x00 0x00 0x00 0x1d]                                       # length = 29
      [0x02]                                                      # dof = 2
      [0x00]                                                      # type = AMQP (0)
      [0x00 0x01]                                                 # channel = 1
      [0x00 0x53 0x11]                                            # performative = begin (17)
      [0xc0 0x10]                                                 # list type (list8), physical length = 16
      [0x04]                                                      # number of fields in list = 4
      [0x60 0x00 0x01]                                            # remote-channel = 1
      [0x52 0x01]                                                 # next-outgoing-id = 1
      [0x70 0x00 0x00 0x3f 0xff]                                  # incoming-window = 16383
      [0x70 0x7f 0xff 0xff 0xff]                                  # outgoing-window = 2147483647

# attach frame exchange
read [0x00 0x00 0x00 0x1d]                                        # length = 29
     [0x02]                                                       # dof = 2
     [0x00]                                                       # type = AMQP (0)
     [0x00 0x01]                                                  # channel = 1
     [0x00 0x53 0x12]                                             # performative = attach (18)
     [0xd0 0x00 0x00 0x00 0x0d]                                   # list type (list32), physical length = 13
     [0x00 0x00 0x00 0x03]                                        # number of fields in list = 3
     [0xa1 0x05] "link1"                                          # name = "link1"
     [0x43]                                                       # handle = 0
     [0x42]                                                       # role = sender

write [0x00 0x00 0x00 0x1d]                                       # length = 29
      [0x02]                                                      # dof = 2
      [0x00]                                                      # type = AMQP (0)
      [0x00 0x01]                                                 # channel = 1
      [0x00 0x53 0x12]                                            # performative = attach (18)
      [0xd0 0x00 0x00 0x00 0x0d]                                  # list type (list32), physical length = 13
      [0x00 0x00 0x00 0x03]                                       # number of fields in list = 3
      [0xa1 0x05] "link1"                                         # name = "link1"
      [0x43]                                                      # handle = 0
      [0x41]                                                      # role = receiver

# flow frame
write [0x00 0x00 0x00 0x1c]                                       # length = 28
      [0x02]                                                      # dof = 2
      [0x00]                                                      # type = AMQP (0)
      [0x00 0x01]                                                 # channel = 1
      [0x00 0x53 0x13]                                            # performative = flow (19)
      [0xc0 0x0f]                                                 # list type (list8), physical length = 15
      [0x04]                                                      # number of fields in list = 4
      [0x52 0x01]                                                 # next-incoming-id = 1
      [0x70 0x00 0x00 0x3f 0xff]                                  # incoming-window = 16383
      [0x52 0x01]                                                 # next-outgoing-id = 1
      [0x70 0x7f 0xff 0xff 0xff]                                  # incoming-window = 2147483647

# transfer frame
read [0x00 0x00 0x00 0x15]                                       # length = 21
     [0x02]                                                      # dof = 2
     [0x00]                                                      # type = AMQP (0)
     [0x00 0x01]                                                 # channel = 1
     [0x00 0x53 0x14]                                            # performative = transfer (20)
     [0xc0 0x08]                                                 # list type (list8), physical length = 8
     [0x05]                                                      # number of fields in list = 1
     [0x43]                                                      # handle = 0
     [0x43]                                                      # delivery-id = 0
     [0xa0 0x01 0x00]                                            # delivery-tag = 00
     [0x43]                                                      # message-format = 0
     [0x41]                                                      # settled = true
