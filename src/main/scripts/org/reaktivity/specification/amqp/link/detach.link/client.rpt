connect await ROUTED_SERVER
        "nukleus://streams/amqp#0"
  option nukleus:window 8192
  option nukleus:transmission "duplex"

connected

# header exchange
write "AMQP" [0x00 0x01 0x00 0x00]
read "AMQP" [0x00 0x01 0x00 0x00]

# open frame exchange
write [0x00 0x00 0x00 0x18]                                      # length = 24
      [0x02]                                                     # dof = 2
      [0x00]                                                     # type = AMQP (0)
      [0x00 0x00]                                                # channel = 0
      [0x00 0x53 0x10]                                           # performative = open (16)
      [0xd0 0x00 0x00 0x00 0x08]                                 # list type (list32), physical length = 8
      [0x00 0x00 0x00 0x01]                                      # number of fields in list = 1
      [0xa1 0x02] "c1"                                           # container-id = "c1"

read [0x00 0x00 0x00 0x19]                                       # length = 25
     [0x02]                                                      # dof = 2
     [0x00]                                                      # type = AMQP (0)
     [0x00 0x00]                                                 # channel = 0
     [0x00 0x53 0x10]                                            # performative = open (16)
     [0xc0 0x0c]                                                 # list type (list8), physical length = 12
     [0x01]                                                      # number of fields in list = 1
     [0xa1 0x09] "localhost"                                     # container-id = "localhost"

# begin frame exchange
write [0x00 0x00 0x00 0x1b]                                      # length = 27
      [0x02]                                                     # dof = 2
      [0x00]                                                     # type = AMQP (0)
      [0x00 0x01]                                                # channel = 1
      [0x00 0x53 0x11]                                           # performative = begin (17)
      [0xc0 0x0e]                                                # list type (list8), physical length = 14
      [0x04]                                                     # number of fields in list = 4
      [0x40]                                                     # remote-channel = null
      [0x52 0x01]                                                # next-outgoing-id = 1
      [0x70 0x00 0x00 0x07 0xff]                                 # incoming-window = 2047
      [0x70 0x7f 0xff 0xff 0xff]                                 # outgoing-window = 2147483647

read [0x00 0x00 0x00 0x1d]                                       # length = 29
     [0x02]                                                      # dof = 2
     [0x00]                                                      # type = AMQP (0)
     [0x00 0x01]                                                 # channel = 1
     [0x00 0x53 0x11]                                            # performative = begin (17)
     [0xc0 0x10]                                                 # list type (list8), physical length = 16
     [0x04]                                                      # number of fields in list = 4
     [0x60 0x00 0x01]                                            # remote-channel = 1
     [0x52 0x01]                                                 # next-outgoing-id = 1
     [0x70 0x00 0x00 0x3f 0xff]                                  # incoming-window = 16383
     [0x70 0x7f 0xff 0xff 0xff]                                  # outgoing-window = 2147483647

# attach frame exchange
write [0x00 0x00 0x00 0x30]                                      # length = 48
      [0x02]                                                     # dof = 2
      [0x00]                                                     # type = AMQP (0)
      [0x00 0x01]                                                # channel = 1
      [0x00 0x53 0x12]                                           # performative = attach (18)
      [0xc0 0x23]                                                # list type (list8), physical length = 35
      [0x06]                                                     # number of fields in list = 6
      [0xa1 0x05] "link1"                                        # name = "link1"
      [0x43]                                                     # handle = 0
      [0x41]                                                     # role = receiver
      [0x50 0x01]                                                # snd-settle-mode = settled
      [0x50 0x00]                                                # rcv-settle-mode = first
      [0x00 0x53 0x28]                                           # source
      [0xc0 0x10]                                                # list type (list8), physical length = 16
      [0x01]                                                     # number of fields in list = 1
      [0xa1 0x0d] "queue://queue"                                # address = "queue://queue"

read [0x00 0x00 0x00 0x30]                                       # length = 48
     [0x02]                                                      # dof = 2
     [0x00]                                                      # type = AMQP (0)
     [0x00 0x01]                                                 # channel = 1
     [0x00 0x53 0x12]                                            # performative = attach (18)
     [0xc0 0x23]                                                 # list type (list8), physical length = 35
     [0x06]                                                      # number of fields in list = 6
     [0xa1 0x05] "link1"                                         # name = "link1"
     [0x43]                                                      # handle = 0
     [0x42]                                                      # role = sender
     [0x50 0x01]                                                 # snd-settle-mode = settled
     [0x50 0x00]                                                 # rcv-settle-mode = first
     [0x00 0x53 0x28]                                            # source
     [0xc0 0x10]                                                 # list type (list8), physical length = 16
     [0x01]                                                      # number of fields in list = 1
     [0xa1 0x0d] "queue://queue"                                 # address = "queue://queue"

# flow frame
write [0x00 0x00 0x00 0x1c]                                      # length = 28
      [0x02]                                                     # dof = 2
      [0x00]                                                     # type = AMQP (0)
      [0x00 0x01]                                                # channel = 1
      [0x00 0x53 0x13]                                           # performative = flow (19)
      [0xc0 0x0f]                                                # list type (list8), physical length = 15
      [0x04]                                                     # number of fields in list = 4
      [0x52 0x01]                                                # next-incoming-id = 1
      [0x70 0x00 0x00 0x07 0xff]                                 # incoming-window = 2047
      [0x52 0x01]                                                # next-outgoing-id = 1
      [0x70 0x7f 0xff 0xff 0xff]                                 # incoming-window = 2147483647

# detach frame
write [0x00 0x00 0x00 0x10]                                      # length = 16
      [0x02]                                                     # dof = 2
      [0x00]                                                     # type = AMQP (0)
      [0x00 0x01]                                                # channel = 1
      [0x00 0x53 0x16]                                           # performative = detach (22)
      [0xc0 0x03]                                                # list type (list8), physical length = 3
      [0x02]                                                     # number of fields in list = 2
      [0x43]                                                     # handle = 0
      [0x41]                                                     # closed = true

read [0x00 0x00 0x00 0x10]                                       # length = 16
     [0x02]                                                      # dof = 2
     [0x00]                                                      # type = AMQP (0)
     [0x00 0x01]                                                 # channel = 1
     [0x00 0x53 0x16]                                            # performative = detach (22)
     [0xc0 0x03]                                                 # list type (list8), physical length = 3
     [0x02]                                                      # number of fields in list = 2
     [0x43]                                                      # handle = 0
     [0x41]                                                      # closed = true
